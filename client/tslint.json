{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [true, "single"],
    "no-implicit-dependencies": [true, "dev"],
    "object-literal-sort-keys": false,
    "interface-name": false,
    "arrow-parens": false
  },
  "linterOptions": {
      "exclude": [
          "spec/fixtures/*.ts"
      ]
  }
}
